{
  "name": "feedback-tracker",
  "version": "1.0.0",
  "description": "Aplicaci√≥n para seguimiento y feedback de errores",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder --win portable"
  },
  "author": "Chuecc",
  "license": "MIT",
  "dependencies": {
    "echarts": "^5.4.3",
    "electron-store": "^8.1.0",
    "ffmpeg-static": "^5.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "electron": "^28.2.1",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.feedback.tracker",
    "productName": "Feedback Tracker",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "assets/icons/app-icon.ico",
      "publisherName": "IB Scope"
    },
    "files": [
      "**/*"
    ],
    "asar": true,
    "forceCodeSigning": false,
    "compression": "maximum"
  }
}
